@model List<Borrower>
@{
    ViewBag.Title = "Borrower Manager";
}
<h1>@ViewBag.Title</h1>
<table class="table table-dark">
    <thead>
        <tr>
            <th>Borrower Id</th>
            <th>Borrower Last Name</th>
            <th>Borrower First Name</th>
            <th>Borrower Phone Number</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var borrower in Model)
        {
        <tr>
            <td>@borrower.BorrowerId</td>
            <td>@borrower.BorrowerLname</td>
            <td>@borrower.BorrowerFname</td>
            <td>@borrower.BorrowerPhone</td>
        </tr>
        }
    </tbody>
</table>
